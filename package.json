    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0",
    "keyv": "^4.0.0",
    "keyv-file": "^0.1.13",
    "lodash": "^4.17.15",
    "nicely-format": "^1.1.0",
    "osc": "^2.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
